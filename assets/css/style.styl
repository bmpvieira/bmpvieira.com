@import 'nib'

/*
 * Non-responsive layout settings
 */

@import 'bootstrap.min.css'

html
  height 100%
  background unquote('url(data:image/png;base64,)')
  background-color #666

body
  //height 100% this conflicts with twitter's scrollspy
  background transparent
  font-family "Ubuntu", "Helvetica Neue", Helvetica, Arial, sans-serif
  padding-top 50px

p
  font-family 'Droid Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif

h1
  text-align center

.navbar-inner
  background black
  background linear-gradient(top,  rgba(255,255,255,0.9) 0%, rgba(246,246,246,0.9) 47%, rgba(237,237,237,0.9) 100%)

.container-fluid .content
  background white
  background linear-gradient(top,  rgba(255,255,255,0.9) 0%, rgba(246,246,246,0.9) 47%, rgba(237,237,237,0.9) 100%)
  border 1px solid #999999
  border-radius bottom 14px
  box-shadow 0px 5px 5px #000000
  margin-bottom 10px
  padding 20px

article aside 
  margin-left 0
  padding-left 19px
  border-left 1px solid #aaa
  text-align center

h1
  // To fix subnav scrolling
  //padding-top 100px

footer
  color #111
  border-top 1px solid #666
  padding-top 5px
  padding-bottom 0px
  font-size .8em
  text-align center
  a
    color #022b4f

// Twitter Boostrap

.navbar-fixed-top .container, .navbar-fixed-bottom .container
  width 940px

.nav-pills > .active > a, .nav-pills > .active > a:hover
  background-color: #666;

.carousel-inner
  text-align center

.carousel .item > img
  width 100%

.thumbnail > img
  height 150px

.thumbnails
  text-align center

.thumbnails > li
  display inline-block 
  float none

.nav-collapse .nav > li > a.build,
.nav-collapse .dropdown-menu a.build 
  color #666

.affix
  top 40px
  background-color #eee
  opacity 0.95
  width 70%


/*
 * Responsive layout settings
 */

@import 'bootstrap-responsive.min.css'

// Twitter bootstrap settings
.navbar-fixed-top
  margin-bottom 0px


/*
 * Media queries
 */

@media (max-width: 480px)
  .affix
    display none

@media (max-width: 980px)
  .affix
    top 0px

@media (min-width: 980px)
  // to center navbar
  .navbar-inner .container
    width 980px
  .container-fluid
    width 980px
    margin 0 auto